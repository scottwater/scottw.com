    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "scripts": {
    "build": "run-s -l clean package:prod generate purge",
    "build:serve": "run-s -l clean package:prod generate purge serve:dist",
    "clean": "rm -rf dist",
    "clean:assets": "rm -rf dist/assets",
    "dev": "run-p -l package serve",
    "generate": "eleventy",
    "package": "webpack --mode development --watch --env NODE_ENV=development",
    "package:prod": "webpack --mode production --env NODE_ENV=production",
    "purge": "purgecss --config purgecss.config.js --output dist/assets",
    "serve": "eleventy --serve",
    "serve:dist": "npx http-server dist",
    "new_post": "node bin/new_post.js",
    "new_board": "node bin/new_board.js"
  },
  "dependencies": {
    "autoprefixer": "^10.0.2",
    "postcss": "^8.1.10",
    "postcss-import": "^13.0.0",
    "postcss-loader": "^4.1.0",
    "tailwindcss": "^2.0.1",
    "@tailwindcss/aspect-ratio": "^0.2.0",
    "@tailwindcss/forms": "^0.2.1",
    "@tailwindcss/typography": "^0.3.1"
  }
