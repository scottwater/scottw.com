    "file-loader": "^6.2.0",
    "html-minifier": "^4.0.0",
    "html-webpack-plugin": "^4.5.0",
    "luxon": "^1.25.0",
    "markdown-it-emoji": "^2.0.0",
    "markdown-it-footnote": "^3.0.2",
    "mini-css-extract-plugin": "^1.3.1",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "purgecss": "^3.0.0",
    "sass-loader": "^10.1.0",
